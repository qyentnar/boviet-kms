<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ruoyi.kms.mapper.KmsAttachmentTypeMapper">
    
    <resultMap type="KmsAttachmentType" id="KmsAttachmentTypeResult">
        <result property="id"    column="id"    />
        <result property="name"    column="name"    />
        <result property="delFlag"    column="del_flag"    />
        <result property="createBy"    column="create_by"    />
        <result property="createTime"    column="create_time"    />
        <result property="updateBy"    column="update_by"    />
        <result property="updateTime"    column="update_time"    />
        <result property="remark"    column="remark"    />
    </resultMap>

    <sql id="selectKmsAttachmentTypeVo">
        select id, name, del_flag, create_by, create_time, update_by, update_time, remark from kms_attachment_type
    </sql>

    <select id="selectKmsAttachmentTypeList" parameterType="KmsAttachmentType" resultMap="KmsAttachmentTypeResult">
        <include refid="selectKmsAttachmentTypeVo"/>
        <where>  
            <if test="name != null  and name != ''"> and name like concat('%', #{name}, '%')</if>
        </where>
    </select>
    
    <select id="selectKmsAttachmentTypeById" parameterType="Long" resultMap="KmsAttachmentTypeResult">
        <include refid="selectKmsAttachmentTypeVo"/>
        where id = #{id}
    </select>
        
    <insert id="insertKmsAttachmentType" parameterType="KmsAttachmentType" useGeneratedKeys="true" keyProperty="id">
        insert into kms_attachment_type
        <trim prefix="(" suffix=")" suffixOverrides=",">
            <if test="name != null">name,</if>
            <if test="delFlag != null">del_flag,</if>
            <if test="createBy != null">create_by,</if>
            <if test="createTime != null">create_time,</if>
            <if test="updateBy != null">update_by,</if>
            <if test="updateTime != null">update_time,</if>
            <if test="remark != null">remark,</if>
         </trim>
        <trim prefix="values (" suffix=")" suffixOverrides=",">
            <if test="name != null">#{name},</if>
            <if test="delFlag != null">#{delFlag},</if>
            <if test="createBy != null">#{createBy},</if>
            <if test="createTime != null">#{createTime},</if>
            <if test="updateBy != null">#{updateBy},</if>
            <if test="updateTime != null">#{updateTime},</if>
            <if test="remark != null">#{remark},</if>
         </trim>
    </insert>

    <update id="updateKmsAttachmentType" parameterType="KmsAttachmentType">
        update kms_attachment_type
        <trim prefix="SET" suffixOverrides=",">
            <if test="name != null">name = #{name},</if>
            <if test="delFlag != null">del_flag = #{delFlag},</if>
            <if test="createBy != null">create_by = #{createBy},</if>
            <if test="createTime != null">create_time = #{createTime},</if>
            <if test="updateBy != null">update_by = #{updateBy},</if>
            <if test="updateTime != null">update_time = #{updateTime},</if>
            <if test="remark != null">remark = #{remark},</if>
        </trim>
        where id = #{id}
    </update>

    <delete id="deleteKmsAttachmentTypeById" parameterType="Long">
        delete from kms_attachment_type where id = #{id}
    </delete>

    <delete id="deleteKmsAttachmentTypeByIds" parameterType="String">
        delete from kms_attachment_type where id in 
        <foreach item="id" collection="array" open="(" separator="," close=")">
            #{id}
        </foreach>
    </delete>
</mapper>