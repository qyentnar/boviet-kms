<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ruoyi.kms.mapper.KmsMainRoleMapper">
    
    <resultMap type="KmsMainRole" id="KmsMainRoleResult">
        <result property="id"    column="id"    />
        <result property="groupId"    column="group_id"    />
        <result property="userIds"    column="user_ids"    />
        <result property="kmsIds"    column="kms_ids"    />
        <result property="delFlag"    column="del_flag"    />
        <result property="createBy"    column="create_by"    />
        <result property="createTime"    column="create_time"    />
        <result property="updateBy"    column="update_by"    />
        <result property="updateTime"    column="update_time"    />
        <result property="remark"    column="remark"    />
    </resultMap>

    <sql id="selectKmsMainRoleVo">
        select id, group_id, user_ids, kms_ids, del_flag, create_by, create_time, update_by, update_time, remark from kms_main_role
    </sql>

    <select id="selectKmsMainRoleList" parameterType="KmsMainRole" resultMap="KmsMainRoleResult">
        <include refid="selectKmsMainRoleVo"/>
        <where>  
            <if test="groupId != null "> and group_id = #{groupId}</if>
            <if test="userIds != null  and userIds != ''"> and user_ids = #{userIds}</if>
            <if test="kmsIds != null  and kmsIds != ''"> and kms_ids = #{kmsIds}</if>
        </where>
    </select>
    
    <select id="selectKmsMainRoleById" parameterType="Long" resultMap="KmsMainRoleResult">
        <include refid="selectKmsMainRoleVo"/>
        where id = #{id}
    </select>
        
    <insert id="insertKmsMainRole" parameterType="KmsMainRole" useGeneratedKeys="true" keyProperty="id">
        insert into kms_main_role
        <trim prefix="(" suffix=")" suffixOverrides=",">
            <if test="groupId != null">group_id,</if>
            <if test="userIds != null">user_ids,</if>
            <if test="kmsIds != null">kms_ids,</if>
            <if test="delFlag != null">del_flag,</if>
            <if test="createBy != null">create_by,</if>
            <if test="createTime != null">create_time,</if>
            <if test="updateBy != null">update_by,</if>
            <if test="updateTime != null">update_time,</if>
            <if test="remark != null">remark,</if>
         </trim>
        <trim prefix="values (" suffix=")" suffixOverrides=",">
            <if test="groupId != null">#{groupId},</if>
            <if test="userIds != null">#{userIds},</if>
            <if test="kmsIds != null">#{kmsIds},</if>
            <if test="delFlag != null">#{delFlag},</if>
            <if test="createBy != null">#{createBy},</if>
            <if test="createTime != null">#{createTime},</if>
            <if test="updateBy != null">#{updateBy},</if>
            <if test="updateTime != null">#{updateTime},</if>
            <if test="remark != null">#{remark},</if>
         </trim>
    </insert>

    <update id="updateKmsMainRole" parameterType="KmsMainRole">
        update kms_main_role
        <trim prefix="SET" suffixOverrides=",">
            <if test="groupId != null">group_id = #{groupId},</if>
            <if test="userIds != null">user_ids = #{userIds},</if>
            <if test="kmsIds != null">kms_ids = #{kmsIds},</if>
            <if test="delFlag != null">del_flag = #{delFlag},</if>
            <if test="createBy != null">create_by = #{createBy},</if>
            <if test="createTime != null">create_time = #{createTime},</if>
            <if test="updateBy != null">update_by = #{updateBy},</if>
            <if test="updateTime != null">update_time = #{updateTime},</if>
            <if test="remark != null">remark = #{remark},</if>
        </trim>
        where id = #{id}
    </update>

    <delete id="deleteKmsMainRoleById" parameterType="Long">
        delete from kms_main_role where id = #{id}
    </delete>

    <delete id="deleteKmsMainRoleByIds" parameterType="String">
        delete from kms_main_role where id in 
        <foreach item="id" collection="array" open="(" separator="," close=")">
            #{id}
        </foreach>
    </delete>
</mapper>